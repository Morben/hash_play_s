        <script>
          swiper.slideTo(2,1000,false);
          function clickswiper(index){
            if(index == 2){
                //牛牛
                document.getElementById("niuniuex").style.display = "block";
                document.getElementById("weishuex").style.display = "none";
                document.getElementById("danshuangex").style.display = "none";
                document.getElementById("baijialeex").style.display = "none";
                
                document.getElementById("niuniurule").style.display = "block";
                document.getElementById("shuangweirule").style.display = "none";
                document.getElementById("danshuangrule").style.display = "none";
                document.getElementById("baijialerule").style.display = "none";
            }else if(index == 3){
                //尾数
                document.getElementById("niuniuex").style.display = "none";
                document.getElementById("weishuex").style.display = "block";
                document.getElementById("danshuangex").style.display = "none";
                document.getElementById("baijialeex").style.display = "none";
                
                document.getElementById("niuniurule").style.display = "none";
                document.getElementById("shuangweirule").style.display = "block";
                document.getElementById("danshuangrule").style.display = "none";
                document.getElementById("baijialerule").style.display = "none";
            }else if(index == 4){
                //单双
                document.getElementById("niuniuex").style.display = "none";
                document.getElementById("weishuex").style.display = "none";
                document.getElementById("danshuangex").style.display = "block";
                document.getElementById("baijialeex").style.display = "none";
                
                document.getElementById("niuniurule").style.display = "none";
                document.getElementById("shuangweirule").style.display = "none";
                document.getElementById("danshuangrule").style.display = "block";
                document.getElementById("baijialerule").style.display = "none";
            }else if(index == 5){
                //百家乐
                 document.getElementById("niuniuex").style.display = "none";
                document.getElementById("weishuex").style.display = "none";
                document.getElementById("danshuangex").style.display = "none";
                document.getElementById("baijialeex").style.display = "block";
                
                document.getElementById("niuniurule").style.display = "none";
                document.getElementById("shuangweirule").style.display = "none";
                document.getElementById("danshuangrule").style.display = "none";
                document.getElementById("baijialerule").style.display = "block";
            }else{
                
            }
            swiper.slideTo(index,1000,false);
          }   
           

